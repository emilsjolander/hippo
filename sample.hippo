
# type definition
(type vec2
	x:float
	y:float)

# function definition
(func dot:float v1:vec2 v2:vec2
	(+ 	(* v1.x v2.x)
		(* v1.y v2.y)))

# implement de-increment operators
(func -:int i:int (- i 1))
(func --:int i:int (- i 2))

# fibonacci function
(func fib:int i:int
	(if (< i 2)
		1
		(+  (fib (-- i))
			(fib (- i)))))

# executed when running script
(print (dot (vec2 1.0 1.0) (vec2 2.0 2.0)))
(print (fib 10))

